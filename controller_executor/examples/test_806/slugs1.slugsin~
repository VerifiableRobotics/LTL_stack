| ! r2_ac | | r4_ac' r2_ac' r3_ac'
| ! r3_ac | | r1_ac' r4_ac' r3_ac'
| ! r4_ac | | r4_ac' r2_ac' r3_ac'

| ! & r1_ac r2 ! r3_ac'
| ! & r1_ac r2 ! r4_ac'
| ! & r1_ac r3 ! r2_ac'
| ! & r1_ac r3 ! r4_ac'
| ! & r2_ac r1 ! r3_ac'
| ! & r2_ac r1 ! r4_ac'
| ! & r2_ac r4 ! r1_ac'
| ! & r2_ac r4 ! r3_ac'
| ! & r3_ac r1 ! r2_ac'
| ! & r3_ac r1 ! r4_ac'
| ! & r3_ac r4 ! r2_ac'
| ! & r3_ac r4 ! r1_ac'
| ! & r4_ac r2 ! r1_ac'
| ! & r4_ac r2 ! r3_ac'
| ! & r4_ac r3 ! r2_ac'
| ! & r4_ac r3 ! r1_ac'

| ! & r1_ac r1 r1_ac'
| ! & r2_ac r2 r2_ac'
| ! & r3_ac r3 r3_ac'
| ! & r4_ac r4 r4_ac'

| | & bigger' ! smaller' & ! bigger' smaller' & ! bigger' ! smaller'
| | | & & & r1_ac' ! r2_ac' ! r3_ac' ! r4_ac' & & & ! r1_ac' r2_ac' ! r3_ac' ! r4_ac' & & & ! r1_ac' ! r2_ac' r3_ac' ! r4_ac' & & & ! r1_ac' ! r2_ac' ! r3_ac' r4_ac'
| | | & & & r1_ac ! r2_ac ! r3_ac ! r4_ac & & & ! r1_ac r2_ac ! r3_ac ! r4_ac & & & ! r1_ac ! r2_ac r3_ac ! r4_ac & & & ! r1_ac ! r2_ac ! r3_ac r4_ac


[ENV_INIT]
bigger
! smaller
r1_ac
! r2_ac
